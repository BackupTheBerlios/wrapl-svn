MOD Target;

IMP IO.Terminal USE Out;

DEF T! <- <[]
>;

VAR queue <- [];

TO :queue(t@T) queue:put(t);

TO :process(t@T, srcdir, incdir, libdir) (
	--Out:writes("Unhandled target: ", t:name, ".\n");
);

DEF Process!(srcdir, incdir, libdir) EVERY queue:values:process(srcdir, incdir, libdir);

END Target.
